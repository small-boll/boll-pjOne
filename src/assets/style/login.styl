.login-template
  width 100%
  height 100%
  position relative
  overflow hidden
  .login-bg
    width 100%
    height 100%
    object-fit fill
  .lt-box
    position absolute
    top 50%
    left 50%
    transform translate(-50%, -52%)
    width 360px
    height 200px
    border 1px solid #fff
    color #fff
